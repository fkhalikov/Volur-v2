{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Mongo": {
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "volur"
  },
  "Eodhd": {
    "ApiToken": "",
    "BaseUrl": "https://eodhd.com/",
    "TimeoutSeconds": 10
  },
  "CacheTtl": {
    "ExchangesHours": 24,
    "SymbolsHours": 24
  },
  "Cors": {
    "AllowedOrigins": [ "http://localhost:5173", "http://localhost:3000" ]
  }
}

